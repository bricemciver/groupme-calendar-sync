{
  "name": "groupme-calendar-sync",
  "version": "0.0.2",
  "description": "Sync GroupMe events to Google Calendar using Google Apps Script",
  "main": "code.ts",
  "scripts": {
    "deploy": "clasp push",
    "watch": "clasp push --watch",
    "open": "clasp open",
    "logs": "clasp logs",
    "pull": "clasp pull",
    "create": "clasp create --type standalone --rootDir ./src",
    "login": "clasp login",
    "type-check": "tsc --noEmit",
    "build": "tsc --noEmit && echo 'TypeScript compilation successful'"
  },
  "keywords": [
    "google-apps-script",
    "groupme",
    "calendar",
    "sync",
    "typescript"
  ],
  "author": "Brice McIver",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.5.0",
    "@types/google-apps-script": "^1.0.97",
    "typescript": "^5.8.3"
  }
}
